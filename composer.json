{
    "name": "liman/liman",
    "description": "Liman System Manager",
    "keywords": ["liman", "linux", "havelsan"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.4",
        "acsystems/keycloak-php-sdk": "^4.4",
        "ankitpokhrel/tus-php": "^2.4",
        "bacon/bacon-qr-code": "^3.0",
        "beyondcode/laravel-websockets": "^1.13",
        "doctrine/dbal": "^4.2",
        "guzzlehttp/guzzle": "^7.9",
        "jackiedo/dotenv-editor": "^2.0",
        "laravel/framework": "^10.0",
        "laravel/helpers": "^1.7",
        "laravel/tinker": "^2.10",
        "laravel/ui": "^4.6",
        "league/csv": "^9.21",
        "mervick/aes-everywhere": "^1.1",
        "phpseclib/phpseclib": "~3.0",
        "pragmarx/google2fa-laravel": "^2.2",
        "pusher/pusher-php-server": "^7.0",
        "stevenmaguire/oauth2-keycloak": "^5.1",
        "tymon/jwt-auth": "^2.1"
    },
    "require-dev": {
        "fakerphp/faker": "^1.24",
        "laravel/pint": "^1.21",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^6.2",
        "phpunit/phpunit": "^12.0",
        "spatie/laravel-ignition": "^2.9",
        "spatie/laravel-web-tinker": "^1.10"
    },
    "autoload": {
        "files": [
            "app/Http/Helpers.php"
        ],
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "files": [
            "app/Http/Helpers.php"
        ],
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/dusk"
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "php artisan package:discover --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "prefer-stable": true
}
